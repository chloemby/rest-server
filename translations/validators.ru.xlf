<?xml version="1.0"?>
<xliff version="1.2" xmlns="urn:oasis:names:tc:xliff:document:1.2">
    <file source-language="en" datatype="plaintext" original="file.ext">
        <body>
            <trans-unit id="1">
                <source>Your password must be at least {{length}} characters long.</source>
                <target>Длина пароля должна быть не менее {{length}} символов.</target>
            </trans-unit>
            <trans-unit id="2">
                <source>Your password must include at least one letter.</source>
                <target>Пароль должен содержать, по крайней мере, одну букву.</target>
            </trans-unit>
            <trans-unit id="3">
                <source>Your password must include both upper and lower case letters.</source>
                <target>Пароль должен содержать как прописные, так и строчные буквы.</target>
            </trans-unit>
            <trans-unit id="4">
                <source>Your password must include at least one number.</source>
                <target>Пароль должен содержать, по крайней мере, одну цифру.</target>
            </trans-unit>
            <trans-unit id="5">
                <source>Your password must contain at least one special character.</source>
                <target>Пароль должен содержать, по крайней мере, один специальный символ.</target>
            </trans-unit>
            <trans-unit id="6">
                <source>password_too_weak</source>
                <target>Пароль должен быть как минимум "{{ min_strength }}" уровня сложности, текущий уровень сложности "{{ current_strength }}", обратите внимание на эти советы: {{ strength_tips }}.</target>
            </trans-unit>

            <!-- Strength levels -->
            <trans-unit id="7">
                <source>rollerworks_password.strength_level.very_weak</source>
                <target>Очень слабый</target>
            </trans-unit>
            <trans-unit id="8">
                <source>rollerworks_password.strength_level.weak</source>
                <target>Слабый</target>
            </trans-unit>
            <trans-unit id="9">
                <source>rollerworks_password.strength_level.medium</source>
                <target>Средний</target>
            </trans-unit>
            <trans-unit id="10">
                <source>rollerworks_password.strength_level.strong</source>
                <target>Сильный</target>
            </trans-unit>
            <trans-unit id="11">
                <source>rollerworks_password.strength_level.very_strong</source>
                <target>Очень сильный</target>
            </trans-unit>

            <!-- Tip translations -->
            <trans-unit id="12">
                <source>rollerworks_password.tip.letters</source>
                <target>добавить буквы (прописные / строчные)</target>
            </trans-unit>
            <trans-unit id="13">
                <source>rollerworks_password.tip.numbers</source>
                <target>добавить цифры</target>
            </trans-unit>
            <trans-unit id="14">
                <source>rollerworks_password.tip.lowercase_letters</source>
                <target>добавить строчные буквы</target>
            </trans-unit>
            <trans-unit id="15">
                <source>rollerworks_password.tip.uppercase_letters</source>
                <target>добавить заглавные буквы</target>
            </trans-unit>
            <trans-unit id="16">
                <source>rollerworks_password.tip.special_chars</source>
                <target>добавить специальные символы</target>
            </trans-unit>
            <trans-unit id="17">
                <source>rollerworks_password.tip.length</source>
                <target>добавить больше символов</target>
            </trans-unit>

            <!-- P0wnedPassword -->
            <trans-unit id="18">
                <source>This password was found in a database of compromised passwords. It has been used {{ used }} times. For security purposes you must use something else.</source>
                <target>Этот пароль был найден в базе данных с взломанными паролями. Он использовался {{used}} раз. В целях безопасности вы должны использовать что-то еще.</target>
            </trans-unit>
        </body>
    </file>
</xliff>